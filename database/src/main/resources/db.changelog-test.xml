<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

	<changeSet id="Person" author="mcichosz" context="test" runAlways="true" runOnChange="true">
		<delete tableName="person"/>
		<loadUpdateData tableName="person" file="test/Person.csv" primaryKey="id" />
	</changeSet>

	<changeSet id="Complex" author="mcichosz" context="test" runAlways="true" runOnChange="true">
		<delete tableName="complex"/>
		<loadUpdateData tableName="complex" file="test/Complex.csv" primaryKey="id" />
	</changeSet>

</databaseChangeLog>
